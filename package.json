{
  "name": "@snowcoders/react-unstyled-input",
  "version": "0.1.1",
  "keywords": [
    "Unstyled",
    "Input"
  ],
  "private": false,
  "repository": "https://github.com/snowcoders/react-unstyled-input",
  "main": "dist/component.js",
  "types": "dist/component.d.ts",
  "scripts": {
    "build": "npm run build-scss && npm run build-typescript",
    "build-scss": "node-sass src/styles.scss dist/styles.css",
    "build-typescript": "tsc",
    "clean": "rimraf .nyc_output coverage dist",
    "cover": "nyc npm run test",
    "prepare": "npm run clean && npm run build",
    "test": "npm run mocha",
    "mocha": "mocha --opts .mocha"
  },
  "author": "Snowcoders",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5"
  },
  "peerDependencies": {
    "react": ">=15.4.0 <^16.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.10",
    "@types/classnames": "^2.2.3",
    "@types/enzyme": "^3.1.6",
    "@types/mocha": "^2.2.45",
    "@types/react": "^16.0.31",
    "chai": "^4.1.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "mocha": "^4.1.0",
    "node-sass": "^4.7.2",
    "nyc": "^11.4.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.0",
    "ts-node": "^4.1.0",
    "typescript": "^2.6.2"
  }
}
